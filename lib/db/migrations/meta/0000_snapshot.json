      "checkConstraints": {
        "buyers_full_name_length": {
          "name": "buyers_full_name_length",
          "value": "char_length(\"buyers\".\"full_name\") >= 2"
        },
        "buyers_phone_format": {
          "name": "buyers_phone_format",
          "value": "\"buyers\".\"phone\" ~ '^[0-9]{10,15}$'"
        },
        "buyers_city_check": {
          "name": "buyers_city_check",
          "value": "\"buyers\".\"city\" IN ('Chandigarh','Mohali','Zirakpur','Panchkula','Other')"
        },
        "buyers_property_type_check": {
          "name": "buyers_property_type_check",
          "value": "\"buyers\".\"property_type\" IN ('Apartment','Villa','Plot','Office','Retail')"
        },
        "buyers_bhk_check": {
          "name": "buyers_bhk_check",
          "value": "\"buyers\".\"bhk\" IS NULL OR \"buyers\".\"bhk\" IN ('1','2','3','4','Studio')"
        },
        "buyers_purpose_check": {
          "name": "buyers_purpose_check",
          "value": "\"buyers\".\"purpose\" IN ('Buy','Rent')"
        },
        "buyers_budget_min_check": {
          "name": "buyers_budget_min_check",
          "value": "\"buyers\".\"budget_min\" IS NULL OR \"buyers\".\"budget_min\" >= 0"
        },
        "buyers_budget_max_check": {
          "name": "buyers_budget_max_check",
          "value": "\"buyers\".\"budget_max\" IS NULL OR \"buyers\".\"budget_min\" IS NULL OR \"buyers\".\"budget_max\" >= \"buyers\".\"budget_min\""
        },
        "buyers_timeline_check": {
          "name": "buyers_timeline_check",
          "value": "\"buyers\".\"timeline\" IN ('0-3m','3-6m','>6m','Exploring')"
        },
        "buyers_source_check": {
          "name": "buyers_source_check",
          "value": "\"buyers\".\"source\" IN ('Website','Referral','Walk-in','Call','Other')"
        },
        "buyers_status_check": {
          "name": "buyers_status_check",
          "value": "\"buyers\".\"status\" IN ('New','Qualified','Contacted','Visited','Negotiation','Converted','Dropped')"
        },
        "buyers_notes_length": {
          "name": "buyers_notes_length",
          "value": "\"buyers\".\"notes\" IS NULL OR char_length(\"buyers\".\"notes\") <= 1000"
